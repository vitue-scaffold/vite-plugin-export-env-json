{
  "name": "@w6s/export-env-json",
  "description": "After building, automatically export the json file of the client-side available properties in .env",
  "version": "1.0.0-beta.1",
  "main": "dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npm run plugin:build && npm run example:build",
    "plugin:dev": "tsc -p . --watch",
    "plugin:build": "tsc -p .",
    "example:build": "cd example && npm run build",
    "example:postinstall": "cd example && npm install",
    "release": "script/release.sh"
  },
  "repository": "https://github.com/WorkPlusFE/vite-plugin-export-env-json.git",
  "author": "Hejx <531601727@qq.com>",
  "license": "MIT",
  "keywords": [
    "vite",
    "vite-plugin",
    "workplusfe",
    "export-env-json"
  ],
  "peerDependencies": {
    "vite": ">2.0.0-0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "typescript": "^4.5.0-beta"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^9.0.13",
    "chalk": "^4.1.2",
    "fs-extra": "^10.0.0",
    "vite": "^2.6.13"
  }
}
